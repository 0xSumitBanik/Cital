{
    "name": "Cital",
    "version": "1.0.0",
    "description": "Easily Generate Citations by entering the DOI of a paper",
    "manifest_version": 2,
    "icons": {
        "16": "files/img/icon-16.png",
        "32": "files/img/icon-32.png"
    },
    "browser_action": {
        "default_icon": "files/img/icon-32.png",
        "default_popup": "extension.html"
    },

    "background": {
        "scripts": ["files/js/extension.js"]
      }
}